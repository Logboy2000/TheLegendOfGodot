; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="The Legend of Godot"
config/version="0.0.1"
run/main_scene="res://scenes/Rooms/Menus/title.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.3", "GL Compatibility")
run/low_processor_mode=true
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://assets/Sprites/charlie.png"
boot_splash/minimum_display_time=500

[autoload]

TransitionLayer="*res://scenes/Autoloads/transition_layer.tscn"
MusicPlayer="*res://scenes/Autoloads/music_player.tscn"
SettingsManager="*res://scenes/Autoloads/settings_manager.gd"
GameManager="*res://scenes/Autoloads/game_manager.tscn"
DebugMenu="*res://scenes/Autoloads/debug_menu.tscn"
PlayerState="*res://scenes/Autoloads/player_state.gd"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0
mouse_cursor/custom_image="res://assets/Sprites/Mouse.png"

[file_customization]

folder_colors={
"res://!EXPORTS/": "gray",
"res://assets/": "red",
"res://scenes/": "orange",
"res://shaders/": "yellow",
"res://themes/": "green"
}

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[gui]

theme/default_font_antialiasing=0

[importer_defaults]

animation_library={
"animation/fps": 20.0
}

